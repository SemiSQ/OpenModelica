!IF [ nmake /$(MAKEFLAGS) /NOLOGO /f Makefile.sig.w32 ]
!ERROR Could not build signature files.
!ENDIF

!INCLUDE Makefile.common.w32

RML=$(OMDEV)\tools\rml\bin\rml
RMLCFLAGS=-Eplain -ftrace
CFLAGS=/Od /I "./VC7/rmlRuntime/runtime/plain" /D "WIN32" /D "_DEBUG" /D "_CONSOLE"\
	/D "_MBCS" /Gm /EHsc /RTC1 /MTd /TC /Fd"VC7/Debug/omcrml.pdb" /W3 /nologo /Z7

SRCC=	$(SRCMO:.mo=.c)
SRCH=	$(SRCMO:.mo=.h)
OBJS=    $(SRCMO:.mo=.obj)
SIGS=    $(ALLMO:.mo=.sig)
SRZ=	$(ALLMO:.mo=.srz)

.SUFFIXES :
.SUFFIXES : .obj .mo

.PHONY : all

all: VC7\Debug\main.obj VC7\Debug\omcrml.lib

VC7\Debug\omcrml.lib: $(OBJS)
	lib /NOLOGO /OUT:VC7\Debug\omcrml.lib $(OBJS)

.mo.obj:
	$(RML) $(RMLCFLAGS) $<
	$(CC) $(CFLAGS) /c $*.c

VC7\Debug\main.obj: main.obj
	copy /B /Y main.obj VC7\Debug\main.obj

clean:
	del /Q /F $(OBJS) $(SRCC) $(SRCH) $(SIGS) $(SRZ) VC7\Debug\main.obj VC7\Debug\omcrml.lib


