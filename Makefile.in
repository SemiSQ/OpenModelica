.PHONY : all modeq modeqd debug release mosh clean

top_builddir = .

#all : debug
all : release 

debug: modeqd

release: modeq

modeqd:	
	(cd c_runtime; $(MAKE) -f Makefile)
	(cd modeq/rml2sig; $(MAKE) -f Makefile)
	(cd modeq; $(MAKE) -f Makefile debug)

modeq:	
	(cd c_runtime; $(MAKE) -f Makefile)
	(cd modeq/rml2sig; $(MAKE) -f Makefile)
	(cd modeq; $(MAKE) -f Makefile release)
mosh:
	(cd mosh/src; $(MAKE) -f Makefile)


clean:
	(cd c_runtime; $(MAKE) -f Makefile clean)
	(cd modeq/rml2sig; $(MAKE) -f Makefile clean)
	(cd modeq; $(MAKE) -f Makefile clean)
	(cd modeq/modeq_debug; $(MAKE) -f Makefile clean)
	(cd modeq/modeq_release; $(MAKE) -f Makefile clean)
	(cd mosh/src; $(MAKE) -f Makefile clean)


.PRECIOUS: Makefile

Makefile: Makefile.in
	$(top_builddir)/config.status Makefile
