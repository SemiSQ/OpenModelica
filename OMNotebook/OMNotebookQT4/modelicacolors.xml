<settings>
	<type>
		<foreground red="255" green="0" blue="0"/>
		<bold/>
	</type>
 	<keyword>
 		<foreground red="0" green="100" blue="0"/>
 		<bold/>
	</keyword>
 	<functionName>
 		<foreground red="0" green="150" blue="0"/>
 		<background red="200" green="200" blue="200"/>
	</functionName>
 	<constant>
 		<foreground red="0" green="200" blue="0"/>
	</constant>
 	<warning>
 		<foreground red="200" green="0" blue="0"/>
	</warning>
 	<builtIn>
 		<foreground red="150" green="0" blue="0"/>
	</builtIn>
 	<variableName>
 		<foreground red="0" green="0" blue="150"/>
	</variableName>
 	<string>
 		<foreground red="240" green="0" blue="0"/>
	</string>
	<comment>
		<foreground red="200" green="0" blue="0"/>
		<italic/>
	</comment>
</settings>