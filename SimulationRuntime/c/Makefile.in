top_builddir = ../..
builddir_bin=$(top_builddir)/build/bin
builddir_lib=$(top_builddir)/build/lib/omc2
builddir_inc=$(top_builddir)/build/include/omc2

CC := @CC@
CXX := @CXX@
CFLAGS := @CFLAGS@

LIBMAKEFILE = Makefile
INTERACTIVELIBS = "-lpthread"
LIBSENDDATA = @LIBSENDDATA@
QMAKE=@QMAKE@
CONFIG_H=$(top_builddir)/Compiler/runtime/config.h $(top_builddir)/Compiler/runtime/config.unix.h
# Use f2c from the OS itself...
LIBF2C=@LIBF2C@

include Makefile.common

libsendData.a :
	cd ./sendData && $(QMAKE) && $(MAKE) -f Makefile
omdevinstall:


