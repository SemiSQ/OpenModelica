CC = @CC@
ANSI= #-ansi
CFLAGS   := @CFLAGS@ -Wall $(ANSI) -pedantic $(EXTRA_CFLAGS)
CPPFLAGS := @CPPFLAGS@ -Wall $(ANSI) $(EXTRA_CFLAGS) -I. -I.. -I../util/ 
AR = ar -ru
FFLAGS  = -O -fexceptions

include ../Makefile.objs

.PHONY : all clean

all : $(META_OBJS)

$(META_OBJS): $(META_HFILES)

meta_modelica_catch.o : meta_modelica_catch.c
	$(CC) $(CPPFLAGS) $(CFLAGS) -c -o $@ -g $<

clean :
	rm -f $(META_OBJS)

Makefile: Makefile.in
	cd ../../../; ./config.status
