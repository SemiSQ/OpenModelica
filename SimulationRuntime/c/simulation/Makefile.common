include ../Makefile.objs

HFILES = \
simulation_input_xml.h \
simulation_runtime.h \
simulation_modelinfo.h \
simulation_varinfo.h \
options.h \
../linearization/linearize.h \

LIBS = libsim.a

.PHONY : libsim.a clean

all : libsim.a

libsim.a : $(SIM_OBJS) $(HFILES)
	$(AR) $@ $(SIM_OBJS)
	ranlib $@

clean :
	rm -f libsim.a
	rm -f $(SIM_OBJS)
