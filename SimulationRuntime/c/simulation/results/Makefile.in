# - Please leave the UNIX Makefile as it is if you are not using UNIX/Linux
# If you need to add files, etc, you modify Makefile.common - a common file
# for both UNIX/Linux and Windows platforms.

CC = @CC@
ANSI= #-ansi
CFLAGS   := @CFLAGS@ -Wall $(ANSI) -pedantic $(EXTRA_CFLAGS) -mfpmath=sse -c -I..
CPPFLAGS := @CPPFLAGS@ -Wall $(ANSI) $(EXTRA_CFLAGS) -I. -I.. -I../../ -I../solver -I../../util -I../../linearization -I../math-support 
AR = ar -ru
FFLAGS  = -O -fexceptions

OBJS = \
read_csv.o read_matlab4.o simulation_result_csv.o simulation_result_mat.o simulation_result_plt.o 

HFILES = \
read_csv.h \
read_matlab4.h \
simulation_result_csv.h \
simulation_result_mat.h \
simulation_result_plt.h \
simulation_result.h \
simulation_result_empty.h \


LIBS = libresult.a

.PHONY : libresult.a clean

all : libresult.a

libresult.a : $(OBJS) $(HFILES)
	$(AR) $@ $(OBJS)
	ranlib $@

clean :
	rm -f libresult.a
	rm -f $(OBJS)

Makefile: Makefile.in
	cd ../../../../; ./config.status

