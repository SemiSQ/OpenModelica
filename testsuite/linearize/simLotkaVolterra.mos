// 
//  Case for linearization of LotkaVolterra model
//
loadFile("modelLotkaVolterra.mo");
simulate(LotkaVolterra,numberOfIntervals=1); // 1 intervals == 2 values
//linearize
system("LotkaVolterra -l 0");
loadFile("linear_LotkaVolterra.mo");
list(linear_LotkaVolterra);
simulate(linear_LotkaVolterra,method="dassl2");
readFile("output.log"); // Check that output log is emtpy
