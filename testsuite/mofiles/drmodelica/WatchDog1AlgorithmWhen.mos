loadFile("WatchDog1AlgorithmWhen.mo");
simulate(WatchDogSystem1,startTime=0.0, stopTime=10.0,numberOfIntervals=500); // 2 intervals == 3 values
echo(false); // turns of output 
size:=readSimulationResultSize("WatchDogSystem1_res.plt");
res:=readSimulationResult("WatchDogSystem1_res.plt",{WatchDog1.watchdogActive},size); 
x:=res[1,1];
y:=res[1,450];
x:=1000*x;
y:=1000*y;
x:=floor(x);
y:=floor(y);
echo(true); // turns on output
x/1000.0; // rounded value to 3 digits
y/1000.0;
readFile("output.log"); // Check that output log is emtpy
system("rm -rf WatchDogSystem1_* WatchDogSystem1.exe WatchDogSystem1.cpp WatchDogSystem1.makefile WatchDogSystem1.libs WatchDogSystem1.log output.log"); // remove tmp files.
