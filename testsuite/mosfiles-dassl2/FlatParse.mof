// name: FlatParse
// keywords: Parse flat Modelica
// status: correct
//
// Test the ability to parse flat modelica
// 

model FlatParse
  Real x[2].y[2];
  Real x.y[2].z;
  parameter Real x.y=1.0;
equation
  der(x[2].y[1])=1;
  der(x[2].y[2])=-x.y;
  x.y[2].z = 0;
end FlatParse;

