loadFile("FlatTank.mo");
simulate(FlatTank,stopTime=150, tolerance=1e-4, numberOfIntervals=150); // 2 intervals == 3 values
echo(false); // turns of output 

x:=val(h,25.0);
y:=val(h,100.0);
z:=val(h,120.0);
w:=val(h,150.0);
x:=1000*x;
y:=1000*y;
z:=1000*z;
w:=1000*w;
x:=floor(x);
y:=floor(y);
z:=floor(z);
w:=floor(w);
echo(true); // turns on output
x/1000.0; // rounded value to 3 digits
y/1000.0;
z/1000.0;
w/1000.0;

