loadFile("MyPointsInst2.mo");
simulate(MyPointsInst2,numberOfIntervals=2, tolerance=1e-5); // 2 intervals == 3 values
echo(false); // turns of output 
size:=readSimulationResultSize("MyPointsInst2_res.mat");

res:=readSimulationResult("MyPointsInst2_res.mat",{x,y,z},size);
 
x:=res[1];
y:=res[2];
z:=res[3];
x:=1000*x;
y:=1000*y;
z:=1000*z;
x:=floor(x);
y:=floor(y);
z:=floor(z);
echo(true); // turns on output
x/1000.0; // rounded value to 3 digits
y/1000.0;
z/1000.0;

