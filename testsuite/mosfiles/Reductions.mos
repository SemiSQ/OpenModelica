loadFile("Reductions.mo");
simulate(Reductions,numberOfIntervals=1); 
echo(false); // turns of output 

res:=readSimulationResult("Reductions_res.plt",{x,y,z,w},3); 
x:=res[1,1];
y:=res[2,1];
z:=res[3,1];
w:=res[4,1];
x:=1000*x;
y:=1000*y;
z:=1000*z;
w:=1000*w;
x:=floor(x);
y:=floor(y);
z:=floor(z);
w:=floor(w);
echo(true); // turns on output
x/1000.0; // rounded value to 3 digits
y/1000.0;
z/1000.0;
w/1000.0;
readFile("output.log"); // Check that output log is emtpy
system("rm -rf Reductions Reductions.libs Reductions.log Reductions.makefile Reductions.cpp Reductions_* output.log"); // remove tmp files.
