loadFile("WhenEquation.mo");
simulate(WhenEquation,startTime=0.0, stopTime=10.0); // 2 intervals == 3 values
echo(false); // turns of output 

size := readSimulationResultSize("WhenEquation_res.plt");
res:=readSimulationResult("WhenEquation_res.plt",{x,y1,y3},size); 

x1:=res[1,1];
x2:=res[1,size];
y11:=res[2,200];
y12:=res[2,size];
y31:=res[3,1];
y32:=res[3,size];

x1 := 1000*x1;
x2 := 1000*x2;
y11 := 1000*y11;
y12 := 1000*y12;
y31 := 1000*y31;
y32 := 1000*y32;

x1 := floor(x1);
x2 := floor(x2);
y11 := floor(y11);
y12 := floor(y12);
y31 := floor(y31);
y32 := floor(y32);

echo(true); // turns on output
x1/1000.0; // rounded value to 3 digits
x2/1000.0;
y11/1000.0;
y12/1000.0;
y31/1000.0;
y32/1000.0;

readFile("output.log"); // Check that output log is emtpy
system("rm -rf WhenEquation_* WhenEquation.exe WhenEquation.cpp WhenEquation.makefile WhenEquation.libs WhenEquation.log output.log");
